{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "padding": {
            "top": 2
        }
    },
    "modules": [
        "title",
        "separator",
        "os",
        "host",
        "bios",
        "bootmgr",
        "kernel",
        "uptime",
        "processes",
        "packages",
        "shell",
        "editor",
        "wm",
        "theme",
        "font",
        "cursor",
        "terminal",
        {
            "type": "cpu",
            "showPeCoreCount": true,
            "temp": true
        },
        {
            "type": "gpu",
            "driverSpecific": true,
            "temp": true
        },
        "memory",
        "physicalmemory",
        "swap",
        "disk",
        "poweradapter",
        "player",
        {
            "type": "localip",
            "showSpeed": true
        },
        "datetime",
        "bluetooth",
        "bluetoothradio",
        "gamepad",
        "break",
        "colors"
    ]
}